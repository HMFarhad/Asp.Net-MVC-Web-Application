@model FKM_Diagnostic_Center.User

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Details about</h2>
<div class="display-field">
    <h2> @Html.DisplayFor(model => model.username)</h2>
</div>
<fieldset>
    <legend></legend>
   
    <div class="display-field">
        <p>Email: @Html.DisplayFor(model => model.email)</p>
    </div>

    
    <div class="display-field">
        <p>Address: @Html.DisplayFor(model => model.address)</p>
    </div>

    
    <div class="display-field">
        <p>Phone number: @Html.DisplayFor(model => model.phone)</p>
    </div>

    
    <div class="display-field">
        <p>User Type: @Html.DisplayFor(model => model.type)</p>
    </div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.id }) |
    @Html.ActionLink("Back to List", "List")
</p>
